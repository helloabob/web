.listPage{clear:both;margin-top:10px;}
.listPage h2{
	font-size:14px;
	font-weight:bold;
	color:#666;
	line-height:33px;
	height:33px;
	overflow:hidden;
}
.filters{
	clear:both;
	margin-bottom:15px;
}

.filters .inner{clear:both;padding:2px 0;}
.filters .filter-list{
	border:1px solid #ddd;
	display:block;
	line-height:22px;
}
.filters .filter-list li{
	height:22px;
	overflow:hidden;
}
.filters .filter-list li a{display:block;padding:0 10px;color:#666;}
.filters .filter-list li a:hover,
.filters .filter-list li a:active{text-decoration:none;}
.filters .filter-list li.selected a{
	color:#333;
	font-weight:bold;
	background-color:#ebebeb;
}
.filters .filter-list .highlight{
	color:#fff;
	background-color:#4bcaed;
}
.filters .filter-list .highlight a{color:#fff;}
/* -------------
 * Selects
 * ------------- */
.ctrls{clear:both;}
.ctrl-sel{
	float:left;
	display:inline;
	padding:1px 0 4px 0;
}
.ctrl-sel li{
	float:left;
	display:inline;
	margin:0 5px 0 0;
	width:auto;
	position:relative;
	left:0;top:0;
	vertical-align:top;
	z-index:100;
	height:24px;
}
.ctrl-sel li.sel01{width:140px;}
.ctrl-sel li.of{width:13px;line-height:24px;}
.ctrl-sel li.sel03{width:120px;}
.ctrl-sel li.sel01 select{width:135px;}
.ctrl-sel li.sel03 select{width:84px;}

.ctrl-sel li select {border:none;float:left;display:inline;}

.jNiceSelectWrapper {
	border:1px solid #c1c1c1;
	background-color:#fff;
	height: 16px;
	left:0;top:0px;z-index:90;position:absolute;
	padding:3px 0;
}

.ctrl-sel li iframe {position:absolute;z-index:1;left:0;top:0;border:none;}
.ctrl-sel li.sel01 iframe{width:135px;}
.ctrl-sel li.sel03 iframe{width:94px;}
.jNiceSelectWrapper .jNiceSelectText {
	height: 16px;
	line-height: 16px;
	padding: 0 3px;
	border-right: none;
	overflow:hidden;float:none;position:absolute;z-index:100;
}

.jNiceSelectWrapper .jNiceSelectOpen {
	width: 15px;
	height: 16px;
	background: url(../img/icon-select.gif) no-repeat 0 0;
	z-index:99;display:block;position:absolute;right:3px;
}

.jNiceSelectWrapper ul {
	top: 22px;
	background-color: #FFF;
	height: 194px;
	border:1px solid #c1c1c1;
	position:absolute;left:0px;display: none;overflow-y:auto;z-index:100;width: 99%;
}
.jNiceSelectWrapper ul li{white-space:nowrap;display:block;clear:both;}
.jNiceSelectWrapper ul a {
	color:#333;
	display:block;padding:2px 0 2px 2px;text-decoration:none;
}

.jNiceSelectWrapper ul a:hover {color:#fff; background:#4bcaed; }
.jNiceSelectWrapper ul a.selected {color:#333;background: #ebebeb;}
.jNiceFocus { border: dotted 1px #666; }

.jNiceHidden {opacity:0;z-index:-1;position:absolute;}
.jNiceWrapper {vertical-align:top;background-color:#000;}
.jNiceWrapper input {outline:none;moz-outline:none;}
ul.pages{padding:9px 0 3px 0;margin:0;float:right;display:inline;width:auto;}
ul.pages li{float:left;display:block;height:17px;font-size:11px;font-weight:bold;font-family:"Lucida Grande",Helvetica,Arial,Verdana,sans-serif;margin:0;}
ul.pages li.btn{margin-top:2px;width:8px;height:12px;cursor:pointer;}
ul.pages li.first{margin-right:9px;width:12px;background:url("../img/pagination-arrows-first.gif") 0 0 no-repeat;}
ul.pages li.prev{background:url("../img/pagination-arrows-prev.gif") 0 0 no-repeat;}
ul.pages li.next{background:url("../img/pagination-arrows-next.gif") 0 0 no-repeat;}
ul.pages li.first:hover{background-position:0 -15px;}
ul.pages li.prev:hover{background-position:0 -15px;}
ul.pages li.next:hover{background-position:0 -15px;}
ul.pages li.disabled{cursor:default;background-position:0 -30px;}
ul.pages li.disabled:hover{background-position:0 -30px;}
ul.pages li.total{margin:0 13px 0 9px;cursor:pointer;color:#369;}
ul.pages li.total:hover{text-decoration:underline;}
ul.pages li.input{padding:0 10px 0 13px;}
ul.pages input{height:14px;width:11px;border:1px solid #BBB;font-weight:bold;font-size:11px;line-height:11px;padding:0 3px 0 3px;text-align:center;margin:0;font-family:"Lucida Grande",Helvetica,Arial,Verdana,sans-serif;*padding-top:2px;*height:12px;}
ul.pages input.long{width:27px;}
ul.pages input.short{width:11px;}

.listCon{clear:both;border-top:1px solid #ccc;}
.listCon ul.pages{padding: 5px 0;clear:both;}
.listCon .mode-atoz{}
.listCon .mode-atoz-item h2{
	clear:both;
	float:left;display:inline;
	width:28px;
	height:18px;
	text-align:center;
	color:#fff;
	font:16px Arial;
	font-weight:bold;
	background-color:#9a9a9a;
	overflow:hidden;
}
.mode-atoz-item{
	clear:both;
	margin-top:6px;
	width:740px;
	overflow:hidden;
	border-bottom:1px solid #e5e5e5;
}

.mode-atoz-item .show-list{clear:both;padding-top:8px;width:796px;}
.mode-atoz-item .show-list li{
	float:left;
	display:inline;
	height:205px;
	width:134px;
	margin:0 65px 0 0;
}
.mode-atoz-item .show-list li:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
.mode-atoz-item .show-list li a.pic{
	display:block;
	width:130px;
	height:75px;
	overflow:hidden;
	padding:1px;
	margin:0 0 7px 0;
}
.mode-atoz-item .show-list li a.pic img{
	width:130px;
	height:75px;
}
.mode-atoz-item .show-list li .tit{margin-bottom:6px;padding:0 4px;}
.mode-atoz-item .show-list li .tit a{
	color:#666;
	font-weight:bold;
}
.mode-atoz-item .show-list .txt{padding:0 4px;}
.mode-atoz-item .show-list p{color:#999;line-height:18px;}
.mode-atoz-item .show-list p a{color:#666;}
.mode-atoz-item .show-list li span.gray6{color:#666;}